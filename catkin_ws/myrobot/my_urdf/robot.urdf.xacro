<?xml version="1.0" ?>
<robot name="robot">
 
    <link name="base_link">
        <visual>
            <origin xyz=" 0 0 0.06" rpy="0 0 0" />
            <geometry>
                <box size="0.24 0.18 0.08"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 0.4 0 1"/>
            </material>
        </visual>
    </link>
 
    <joint name="base_left_motor_joint" type="fixed">
        <origin xyz="0.055 0.075 0.04" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="left_motor" />
    </joint>
 
    <link name="left_motor">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.0125" length = "0.030"/>
            </geometry>
            <material name="gray">
                <color rgba="0.75 0.75 0.75 1"/>
            </material>
        </visual>
    </link>
 
    <joint name="left_wheel_joint" type="continuous">
        <origin xyz="0 0.0485 0" rpy="0 0 0"/>
        <parent link="left_motor"/>
        <child link="left_wheel_link"/>
        <axis xyz="0 1 0"/>
    </joint>
 
    <link name="left_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.0325" length = "0.017"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 0.9"/>
            </material>
        </visual>
    </link>
 
    <joint name="base_right_motor_joint" type="fixed">
        <origin xyz="0.055 -0.075 0.04" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="right_motor"/>
    </joint>
 
    <link name="right_motor">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.0125" length = "0.030"/>
            </geometry>
            <material name="gray">
                <color rgba="0.75 0.75 0.75 1"/>
            </material>
        </visual>
    </link>
 
    <joint name="right_wheel_joint" type="continuous">
        <origin xyz="0 -0.0485 0" rpy="0 0 0"/>
        <parent link="right_motor"/>
        <child link="right_wheel_link"/>
        <axis xyz="0 1 0"/>
    </joint>
 
    <link name="right_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.0325" length = "0.017"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 0.9"/>
            </material>
        </visual>
    </link>
 
    <joint name="front_castor_joint" type="fixed">
        <origin xyz="-0.1135 0 0.0165" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="front_castor_link"/>
        <axis xyz="0 1 0"/>
    </joint>
 
    <link name="front_castor_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <sphere radius="0.0165"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 0.95"/>
            </material>
        </visual>
    </link>
  
</robot>